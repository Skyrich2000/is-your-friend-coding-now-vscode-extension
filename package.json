{
  "name": "is-your-friend-coding-now",
  "displayName": "is-your-friend-coding-now",
  "description": "is-your-friend-coding-now",
  "version": "0.0.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Skyrich2000/is-your-friend-coding-now-vscode-extension"
  },
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "is-your-friend-coding-now.start",
        "title": "Start share your coding status with your friends"
      }
    ]
  },
  "scripts": {
    "build:package": "pnpm vsce package --no-dependencies",
    "build:publish": "pnpm vsce publish --no-dependencies",
    "package": "webpack --mode production --devtool hidden-source-map",
    "vscode:prepublish": "pnpm run package",
    "compile": "webpack",
    "watch": "webpack --mode development --watch",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "pnpm run compile-tests && pnpm run compile && pnpm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8",
    "eslint": "^8.56.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@octokit/rest": "^20.0.2",
    "axios": "^1.6.5",
    "html-webpack-inline-source-plugin": "^0.0.10",
    "html-webpack-plugin": "^5.6.0",
    "script-ext-html-webpack-plugin": "^2.1.5",
    "vsce": "^2.15.0"
  }
}
